{
    "name": "ferarandrei1/filament-dynamic-settings",
    "description": "Centralized dynamic settings system for Filament",
    "keywords": [
        "filament",
        "laravel",
        "settings",
        "dynamic-settings",
        "configuration",
        "filament-plugin"
    ],
    "homepage": "https://github.com/ferarandrei1/filament-dynamic-settings",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Feraru Andrei",
            "email": "feraruandrei@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^3.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Feraandrei1\\FilamentDynamicSettings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feraandrei1\\FilamentDynamicSettings\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Feraandrei1\\FilamentDynamicSettings\\FilamentDynamicSettingsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
